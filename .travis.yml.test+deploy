sudo: required
services:
 - docker

before_install:
 - docker build -t ayocobb/docker-react -f Dockerfile.dev .

script:
 - docker run ayocobb/docker-react npm run test -- --coverage

deploy:
   provider: elasticbeanstalk
   region: "us-east-1"
   app:  "docker"
   env: "Ayoreact-env" 
   bucket_name: "elasticbeanstalk-us-east-1-956102238953" 
   bucket_path: "docker"
   on:
     branch: master
   access_key_id: $AWS_ACCESS_KEY
   secret_access_key:
     secure: "$AWS_SECRET_KEY"

